{
	"compilerOptions": {
		"baseUrl": ".",
		"exactOptionalPropertyTypes": false,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"moduleResolution": "Bundler",
		"noUncheckedIndexedAccess": false,
		"paths": {
			"~/*": ["src/*"]
		},
		"target": "es2022"
	},
	"extends": "astro/tsconfigs/strictest"
}
